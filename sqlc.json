{
  "version": "1",
  "packages": [{
    "schema": "./db/migrations",
    "queries": "./db/queries",
    "name": "models",
    "path": "./db/models",
    "engine": "postgresql",
    "emit_json_tags": true,
    "emit_prepared_queries": true,
    "emit_interface": true,
    "emit_exact_table_names": false,
    "emit_empty_slices": true
  }]
} 